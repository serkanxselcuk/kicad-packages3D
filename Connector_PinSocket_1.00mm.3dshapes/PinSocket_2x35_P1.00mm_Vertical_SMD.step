copy ReleaseM0BE\Exe\*.a ..\..\..\lib\IAR\ /Y
copy ReleaseM0LE\Exe\*.a ..\..\..\lib\IAR\ /Y
copy ReleaseM3BE\Exe\*.a ..\..\..\lib\IAR\ /Y
